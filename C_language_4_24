/*
#include<stdio.h>

int main(void)
{
	int dan = 0, num = 1;
	printf("몇 단? ");
	scanf_s("%d", &dan);

	while (num < 10)
	{
		printf("%dx%d=%d \n", dan, num, dan * num);
		num++;
	}
	return 0;
}
*/
#include <stdio.h>

int main(void)
{
	int num;
	int i = 0;

	printf("양의 정수 입력: ");
	scanf_s("%d", &num);

	while (i < num)
	{
		printf("Hello world! \n");
		i++;
	}
	return 0;
}
